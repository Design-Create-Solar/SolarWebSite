{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "nginx",
			"image": "docker.io/ishnoor/dcs-nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
            "memory": 128,
            "links": ["api"]
        },
        {
			"name": "api",
			"image": "docker.io/ishnoor/dcs-server",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 5000,
					"containerPort": 5000
				}
			],
            "memory": 128
		},
		{
			"name": "dcsfrontend",
			"image": "docker.io/ishnoor/dcs-frontend", 
			"essential": true,
			"portMappings": [
				{
					"hostPort": 3000,
					"containerPort": 3000
				}
			],
            "memory": 128
		}
	]
}